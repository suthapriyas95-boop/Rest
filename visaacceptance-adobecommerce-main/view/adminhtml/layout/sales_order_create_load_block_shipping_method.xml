<?xml version="1.0"?>
<!-- Load shipping method block for admin order create -->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Shipping\Block\Adminhtml\Order\Create\Shipping\Method" name="cybersource.order.load.shippingmethod" template="order/shipping/method.phtml"/>
        </referenceContainer>
    </body>
</page>
<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="CyberSource\Payment\Block\Adminhtml\Fingerprint" name="cybersource.admin.order.shipping_method" template="CyberSource_Payment::order/shipping_method.phtml"/>
        </referenceContainer>
    </body>
</page>
