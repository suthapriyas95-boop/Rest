<?xml version="1.0"?>
<!-- Load payment method block for admin order create -->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="CyberSource\Payment\Block\Adminhtml\Order\Payment" name="cybersource.order.load.paymentmethod" template="order/payment_method.phtml"/>
        </referenceContainer>
    </body>
</page>
<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="CyberSource\Payment\Block\Adminhtml\Fingerprint" name="cybersource.admin.order.payment_method" template="CyberSource_Payment::order/payment.phtml"/>
        </referenceContainer>
    </body>
</page>
